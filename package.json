{
	"name": "grocery-api",
	"version": "2.2.0",
	"description": "",
	"author": "",
	"license": "MIT",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "nodemon",
		"start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
		"start:prod": "node --max_old_space_size=3500 dist/src/main.js",
		"prestart:prod": "rimraf dist &&  npm run build",
		"create:index": "node create-index.js",
		"postinstall": "npm run prestart:prod",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^6.0.0",
		"@nestjs/core": "^6.0.0",
		"@nestjs/jwt": "^6.1.1",
		"@nestjs/mongoose": "^6.1.2",
		"@nestjs/passport": "^6.1.0",
		"@nestjs/platform-express": "^6.0.0",
		"@nestjs/platform-socket.io": "^6.11.11",
		"@nestjs/swagger": "^3.1.0",
		"@nestjs/websockets": "^6.11.11",
		"@sendgrid/mail": "^7.1.0",
		"@sentry/node": "^5.7.1",
		"@types/bcrypt": "^3.0.0",
		"@types/mongoose": "^5.5.11",
		"@types/nodemailer": "^6.4.0",
		"@types/stripe": "^6.31.20",
		"@types/uuid": "^3.4.5",
		"app-root-path": "^3.0.0",
		"bcrypt": "^5.0.0",
		"class-transformer": "^0.2.3",
		"class-validator": "^0.9.1",
		"dotenv": "^8.0.0",
		"ejs": "^3.1.2",
		"exceljs": "^4.1.1",
		"geolib": "^3.2.1",
		"html-pdf": "^2.2.0",
		"imagekit": "^3.0.4",
		"mongoose": "^5.9.20",
		"node-rsa": "^1.1.1",
		"nodemailer": "^6.4.11",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"request": "^2.88.2",
		"rimraf": "^2.6.2",
		"rxjs": "^6.3.3",
		"sendinblue-apiv3": "^2.0.2",
		"sib-api-v3-sdk": "^8.2.0",
		"stripe": "^7.8.0",
		"swagger-ui-express": "^4.0.7",
		"twilio": "^3.49.0",
		"typescript": "3.8.3",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"@nestjs/testing": "6.1.1",
		"@types/dotenv": "^6.1.1",
		"@types/express": "4.16.1",
		"@types/jest": "24.0.11",
		"@types/node": "11.13.4",
		"@types/socket.io": "^2.1.4",
		"@types/supertest": "2.0.7",
		"jest": "24.7.1",
		"nodemon": "^1.19.1",
		"prettier": "1.17.0",
		"supertest": "4.0.2",
		"ts-jest": "24.0.2",
		"ts-node": "8.1.0",
		"tsc-watch": "2.2.1",
		"tsconfig-paths": "3.8.0",
		"tslint": "5.16.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
